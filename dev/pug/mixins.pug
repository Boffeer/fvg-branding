mixin caseProblem(problem)
	.case-modal-problem
		.case-modal-problem-header

			span.case-modal-problem_number= problem.index

			h4.case-modal-problem_header= problem.title
			.case-modal-problem-name
				p.problem-name= problem.name
		.case-modal-problem_descr
			each val in problem.text
				p= val



mixin caseItemOpen(info)
	.case-modal_wrap(class="case-modal--"+info.number)
		.case-modal
			button(type="button").modal_close-btn ×
			.case-modal_hero
				.case-modal-headliner
					h3.case-modal_headliner Полная упаковка сервиса по аренде авто в Краснодаре
					a.case-modal_link(href="#") legioncar.ru
					p.case-modal_phrase Сотрудничаем с мая 2019 года и по сегодняший день
				img.case-modal_pic(src= info.img)

			.case-modal-bubbles
				.case-modal_bubble.case-modal_bubble__design
					.case-modal_bubble-pic
						img(src="static/img/general/case-design.svg")
					p Разработали дизайн сайта удобный для пользователя
				.case-modal_bubble.case-modal_bubble__seo
					.case-modal_bubble-pic
						img(src="static/img/general/case-seo.svg")
					p Сделали СЕО, настроили контекстную рекламу и получили клиентов из Яндекса и Гугла
				.case-modal_bubble.case-modal_bubble__smm
					.case-modal_bubble-pic
						img(src="static/img/general/case-target.svg")
					p Настроили таргетированную рекламу
				.case-modal_bubble.case-modal_bubble__web
					.case-modal_bubble-pic
						img(src="static/img/general/case-site.svg")
					p Запустили сайт
			h4.case-modal_title Проблемы клиента и как мы их решали



			+caseProblem(info.problemOne)
			+caseProblem(info.problemTwo)


			.case-modal-additional
				p.case-modal-additional_text В таргетированной рекламе акцент сделали на большой выбор автомобилей на все случаи жизни, статусность бренда и возможности, которые дает прокат своим клиентам. Для визуальной составляющей выбирали живые, сочные фотографии и динамичные видео.
				.case-modal-additional-bullets
					h4.case-modal-additional-bullets_title Целевые аудитории:
					.case-modal-additional_bullet__1 Бизнесмены
					.case-modal-additional_bullet__2 Мамы 28+
					.case-modal-additional_bullet__3 Молодые пары
					.case-modal-additional_bullet__4 Семьи
					.case-modal-additional_bullet__5 Офисные сотрудники и др

			.case-modal-results
				h4.case-modal-results_title Результаты
				ul.case-modal-results_list
					li.case-modal-results_item.case-modal-results_item--1 Динамично растущая база постоянных клиентов проката
					li.case-modal-results_item.case-modal-results_item--2 Новый эффективный, лего читаемый сайт, подходящий к запросам клиентов
					li.case-modal-results_item.case-modal-results_item--3 Настроенная контекстная и таргетированная реклама, приносящая прирост клиентов ежемесячно.
					li.case-modal-results_item.case-modal-results_item--4 Разрабатываем мобильное приложение, которое даст клиентам упрощенный вариант использования авто
					li.case-modal-results_item.case-modal-results_item--5 Успешно работаем с жалобами, вопросами аудитории
					li.case-modal-results_item.case-modal-results_item--6 Повышение конверсий на 35% каждый месяц

			.form2
				h2.form-title Чем дольше вы думаете, тем больше денег зарабатывают ваши конкуренты
				.form2-contacts
					.form2-call
						p.form2-cta Позвоните нам
						.phone-numbers
							a.phone-russia(href="") +7 123 456 27 23
							a.phone-belarus(href="") +375 123 456 27 23
					form
						input(name="formname" value='Открытый кейс').g-form-name
						input.g-input(
							name="tel"
							type="tel"
							placeholder="Оставьте номер телефона"
							required)
						input.btn(
							type="submit"
							value="Отправить").btn
						p.gdpr-caption Нажимая кнопку «Отправить», я даю согласие на передачу персональныхданных согласно политике конфиденциальности






// work
mixin workItem(icon, text)
	.work-slider-item
		img(src="static/img/content/7-how/"+icon+".png").work-item__img
		p.work-item__text= text

// reviews
mixin review(name, occupate, text)
	.reviews-slider-item
		.reviews-top
			img(src="https://picsum.photos/200/300").reviews-photo
			.reviews-who
				h3.reviews-name= name
				p.reviews-occupate= occupate
		p.review= text

